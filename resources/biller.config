[global]

app-name=Facturador
repositoryPath=/usr/share/biller-files/
security.password-digest-algoritm=SHA-256
security.session-expiration-minutes=0
default-liquidation-receiver-name=DUMMY

[jpa-biller]

javax.persistence.jdbc.url=jdbc:mysql://xxx.xxx.xxx.xxx:3306/biller?characterEncoding=utf8&amp;useUnicode=true
javax.persistence.jdbc.user=
javax.persistence.jdbc.password=
javax.persistence.jdbc.driver=com.mysql.jdbc.Driver
eclipselink.ddl-generation.output-mode=database
eclipselink.query-results-cache=false
eclipselink.connection-pool.min=5
eclipselink.connection-pool.max=20
eclipselink.logging.level=SEVERE
eclipselink.session.customizer=com.luckia.biller.core.jpa.CustomSessionCustomizer
eclipselink.weaving=static

[jpa-lis]

javax.persistence.jdbc.url=jdbc:mysql://xxx.xxx.xxx.xxx:3306/lis_ht
javax.persistence.jdbc.user=
javax.persistence.jdbc.password=
javax.persistence.jdbc.driver=com.mysql.jdbc.Driver
eclipselink.connection-pool.min=5
eclipselink.connection-pool.max=20

[mail]

host=
user=
password=
fromEmail=
fromName=
port=587
sslConnection=false
tlsConnection=true
